{
  "name": "react-chief-editor",
  "version": "1.0.0-alpha-6",
  "description": "Extendable rich-text editor.",
  "author": "kledk",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kledk/react-chief-editor"
  },
  "files": [
    "dist"
  ],
  "main": "dist/chief-editor.umd.js",
  "module": "dist/chief-editor.es.js",
  "exports": {
    ".": {
      "import": "./dist/chief-editor.es.js",
      "require": "./dist/chief-editor.umd.js"
    }
  },
  "source": "src/index.tsx",
  "engines": {
    "node": ">=10"
  },
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc --noEmit && vite build",
    "start": "rollup -c -w",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "deploy": "gh-pages -d example/build",
    "dev": "vite"
  },
  "dependencies": {
    "@popperjs/core": "^2.6.0",
    "is-hotkey": "^0.2.0",
    "is-url": "^1.2.4",
    "lodash": "^4.17.20",
    "react-overlays": "^4.1.1",
    "react-popper": "^2.2.4",
    "react-resize-detector": "^5.2.0",
    "use-error-boundary": "^2.0.4"
  },
  "peerDependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "slate": "^0.71.0",
    "slate-history": "^0.66.0",
    "slate-react": "^0.71.0",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "styled-components": "^5.2.1",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/is-url": "^1.2.28",
    "@types/react": "^17.0.37",
    "@types/styled-components": "^5.1.5",
    "@vitejs/plugin-react": "^1.1.0",
    "cross-env": "^7.0.2",
    "gh-pages": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rollup": "^2.35.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-typescript2": "^0.29.0",
    "slate": "^0.71.0",
    "slate-history": "^0.66.0",
    "slate-react": "^0.71.0",
    "ts-is-present": "^1.1.1",
    "typescript": "^4.5.2",
    "vite": "^2.6.14",
    "vite-aliases": "^0.8.7"
  }
}
